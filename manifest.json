{
    "manifest_version": 3,
    "name": "Barcode Generator Helper",
    "version": "1.0",
    "description": "Quick access to bulk barcode generator",
    "permissions": [
        "tabs",
        "scripting",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://bulkbarcodegenerator.org/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo.png",
            "48": "logo.png",
            "128": "logo.png"
        }
    },
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
}